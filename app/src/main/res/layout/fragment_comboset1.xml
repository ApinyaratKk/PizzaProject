<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".comboset1"
    android:background="@drawable/bg">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#CA3D3D"
                android:gravity="center"
                android:layout_centerHorizontal="true"
                android:text="@string/comboset1"
                android:textColor="#ECECEC"
                android:textSize="38sp"
                android:textStyle="italic" />


            <ImageView
                android:id="@+id/bbqcpizza"
                android:layout_width="157dp"
                android:layout_height="204dp"
                android:layout_marginStart="35dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="65dp"
                android:layout_marginEnd="35dp"
                android:layout_marginRight="50dp"
                android:src="@drawable/bbqpizza"/>

            <ImageView
                android:id="@+id/textview_beefpasta"
                android:layout_width="155dp"
                android:layout_height="200dp"
                android:layout_marginStart="220dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="65dp"
                android:layout_marginEnd="35dp"
                android:layout_marginRight="15dp"
                android:src="@drawable/beefpasta" />
            <TextView
                android:id="@+id/textview_bbqpizza"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/beefpizza"
                android:layout_marginTop="255dp"
                android:layout_marginStart="80dp"
                android:layout_marginLeft="60dp"
                android:textColor="#ECECEC" />

            <TextView
                android:id="@+id/beefpasta"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/beefpasta"
                android:layout_marginTop="255dp"
                android:layout_marginStart="255dp"
                android:layout_marginLeft="60dp"
                android:textColor="#ECECEC"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="285dp"
                android:layout_marginBottom="470dp">
                <TextView
                    android:id="@+id/textview_beefpizza_price"
                    android:layout_width="61dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="85dp"
                    android:layout_marginLeft="90dp"
                    android:layout_marginEnd="128dp"
                    android:layout_marginRight="200dp"
                    android:text="@string/_199_baht"
                    android:textColor="#ECECEC" />
                <TextView
                    android:id="@+id/textview_beefpasta_price"
                    android:layout_width="61dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="90dp"
                    android:layout_marginEnd="128dp"
                    android:layout_marginRight="200dp"
                    android:text="@string/_89_baht"
                    android:textColor="#ECECEC" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="45dp"
                android:layout_marginLeft="45dp"
                android:layout_marginEnd="45dp"
                android:layout_marginRight="45dp"
                android:layout_marginTop="310dp"
                android:layout_marginBottom="390dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="60dp"
                    android:layout_marginRight="60dp"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/Minusbeefpizza"
                        android:layout_width="24dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginLeft="140dp"
                        android:background="@drawable/btn_gradient"
                        android:text="@string/minus"
                        android:textColor="#ECECEC"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/orderbeefpizza"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginRight="30dp"
                        android:text="@string/_0"
                        android:textSize="20sp"
                        android:textColor="#ECECEC"/>

                    <Button
                        android:id="@+id/Plusbeefpizza"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginBottom="440dp"
                        android:background="@drawable/btn_gradient"
                        android:text="@string/plus"
                        android:textSize="18sp"
                        android:textColor="#ECECEC"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/Minusbeefpasta"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginLeft="140dp"
                        android:background="@drawable/btn_gradient"
                        android:text="@string/minus"
                        android:textSize="20sp"
                        android:textColor="#ECECEC"/>

                    <TextView
                        android:id="@+id/orderbeefpasta"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginRight="30dp"
                        android:text="@string/_0"
                        android:textColor="#ECECEC"
                        android:textSize="20sp" />

                    <Button
                        android:id="@+id/Plusbeefpasta"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginBottom="440dp"
                        android:background="@drawable/btn_gradient"
                        android:text="@string/plus"
                        android:textSize="18sp"
                        android:textColor="#ECECEC"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="35dp"
                android:layout_marginEnd="35dp"
                android:layout_marginTop="330dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/friedchicken_M"
                    android:layout_width="155dp"
                    android:layout_height="200dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="35dp"
                    android:layout_marginRight="50dp"
                    android:src="@drawable/sewwtchick"
                    android:contentDescription="@string/todo" />
                <ImageView
                    android:id="@+id/salmonsalad"
                    android:layout_width="155dp"
                    android:layout_height="200dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="35dp"
                    android:layout_marginRight="50dp"
                    android:src="@drawable/salmonsalad"
                    android:contentDescription="@string/todo" />

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="35dp"
                android:layout_marginLeft="35dp"
                android:layout_marginEnd="35dp"
                android:layout_marginRight="35dp"
                android:layout_marginTop="520dp"
                android:layout_marginBottom="267dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textview_friedchicken_M_price"
                    android:layout_width="155dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginLeft="90dp"
                    android:layout_marginEnd="35dp"
                    android:layout_marginRight="200dp"
                    android:text="@string/fried_chicken_M"
                    android:textColor="#ECECEC" />
                <TextView
                    android:id="@+id/textview_salmonsalad"
                    android:layout_width="155dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="50dp"
                    android:layout_marginEnd="0dp"
                    android:layout_marginRight="200dp"
                    android:text="@string/salmonsalad"
                    android:textColor="#ECECEC" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="550dp"
                android:layout_marginBottom="235dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/friedchicken_M_price"
                    android:layout_width="61dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="85dp"
                    android:layout_marginLeft="90dp"
                    android:layout_marginEnd="128dp"
                    android:layout_marginRight="200dp"
                    android:text="@string/_89_baht"
                    android:textColor="#ECECEC" />
                <TextView
                    android:id="@+id/textview_salmonsalad_price"
                    android:layout_width="61dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="90dp"
                    android:layout_marginEnd="128dp"
                    android:layout_marginRight="200dp"
                    android:text="@string/_99_baht"
                    android:textColor="#ECECEC" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="0dp"
                android:layout_marginRight="100dp"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="570dp"
                android:orientation="horizontal">


                <Button
                    android:id="@+id/Minusfriedchicken_M"
                    android:layout_width="29dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="50dp"
                    android:layout_marginLeft="140dp"
                    android:background="@drawable/btn_gradient"
                    android:text="@string/minus"
                    android:textSize="20sp"
                    android:textColor="#ECECEC"/>

                <TextView
                    android:id="@+id/orderfriedchicken_M"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:text="@string/_0"
                    android:textSize="20sp"
                    android:textColor="#ECECEC"/>

                <Button
                    android:id="@+id/Plusfriedchicken_M"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/btn_gradient"
                    android:text="@string/plus"
                    android:textSize="18sp"
                    android:textColor="#ECECEC"/>


                <Button
                    android:id="@+id/Minussalmonsalad"
                    android:layout_width="29dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="55dp"
                    android:layout_marginLeft="140dp"
                    android:background="@drawable/btn_gradient"
                    android:text="@string/minus"
                    android:textSize="20sp"
                    android:textColor="#ECECEC"/>

                <TextView
                    android:id="@+id/ordersalmonsalad"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:text="@string/_0"
                    android:textSize="20sp"
                    android:textColor="#ECECEC"/>

                <Button
                    android:id="@+id/Plussalmonsalad"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/btn_gradient"
                    android:text="@string/plus"
                    android:textSize="18sp"
                    android:textColor="#ECECEC"/>
            </LinearLayout>

            <Button
                android:id="@+id/backToComFm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/back"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textColor="@color/font_color"
                android:background="@drawable/btn_gradient"
                android:gravity="center"
                android:layout_marginTop="650dp"
                android:layout_marginStart="105dp"/>

            <Button
                android:id="@+id/goToOtherFm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/next"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textColor="@color/font_color"
                android:background="@drawable/btn_gradient"
                android:gravity="center"
                android:layout_marginTop="650dp"
                android:layout_marginStart="205dp"/>

        </RelativeLayout>
    </ScrollView>
</FrameLayout>